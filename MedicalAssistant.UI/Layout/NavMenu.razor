<MudNavMenu Color="Color.Primary">
	<MudNavLink Href="/" Match="NavLinkMatch.All" Icon="@Icons.Material.Outlined.Home">
		@Translations.Currents
	</MudNavLink>
	
	<MudNavGroup Title="@Translations.Visits" Icon="@Icons.Material.Outlined.MedicalServices" Expanded="false">
		<MudNavLink Href="/visits/current" Match="NavLinkMatch.All" Icon="@Icons.Material.Outlined.ViewWeek">
			@Translations.Currents
		</MudNavLink>
		<MudNavLink Href="/visits" Match="NavLinkMatch.All" Icon="@Icons.Material.Outlined.CalendarMonth">
			@Translations.Scheduler
		</MudNavLink>
	</MudNavGroup>
	<MudNavGroup Title="@Translations.Notifications" Icon="@Icons.Material.Outlined.Notifications" Expanded="false">
		<MudNavLink Href="/notification/upcoming" Match="NavLinkMatch.All" Icon="@Icons.Material.Outlined.Upcoming">
			@Translations.Upcoming
		</MudNavLink>
		<MudNavLink Href="/notification?page=1&pagesize=10" Match="NavLinkMatch.All" Icon="@Icons.Material.Outlined.History">
			@Translations.History
		</MudNavLink>

	</MudNavGroup>

</MudNavMenu>