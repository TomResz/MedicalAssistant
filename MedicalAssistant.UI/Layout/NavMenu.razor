<MudNavMenu Color="Color.Primary">
	<MudNavLink Href="/" Match="NavLinkMatch.All" Icon="@Icons.Material.Outlined.Home">
		@Translations.Currents
	</MudNavLink>

	<MudNavGroup Title="@Translations.Medication" Icon="@Icons.Material.Outlined.Medication" Expanded="false">
		<MudNavLink Href="/medication/current" Match="NavLinkMatch.All" Icon="@Icons.Material.Outlined.ViewWeek">
			@Translations.Currents
		</MudNavLink>
		<MudNavLink Href="/medication" Match="NavLinkMatch.All" Icon="@Icons.Material.Outlined.CalendarMonth">
			@Translations.Scheduler
		</MudNavLink>
	</MudNavGroup>


	<MudNavGroup Title="@Translations.Visits" Icon="@Icons.Material.Outlined.MedicalServices" Expanded="false">
		<MudNavLink Href="/visits/current" Match="NavLinkMatch.All" Icon="@Icons.Material.Outlined.ViewWeek">
			@Translations.Currents
		</MudNavLink>
		<MudNavLink Href="/visits" Match="NavLinkMatch.All" Icon="@Icons.Material.Outlined.CalendarMonth">
			@Translations.Scheduler
		</MudNavLink>
	</MudNavGroup>
	
	<MudNavGroup Title="@Translations.Notifications" Icon="@Icons.Material.Outlined.Notifications" Expanded="false">
		<MudNavGroup Title="@Translations.Upcoming" Icon="@Icons.Material.Outlined.Upcoming" Expanded="false">
			<MudNavLink Href="/notification/visit?page=1&pagesize=10"  Match="NavLinkMatch.All" Icon="@Icons.Material.Outlined.MedicalServices">
				@Translations.Visits
			</MudNavLink>
			<MudNavLink Href="/notification/medication?page=1&pagesize=10" Match="NavLinkMatch.All" Icon="@Icons.Material.Outlined.Medication">
				@Translations.Medication
			</MudNavLink>
		</MudNavGroup>
		<MudNavLink Href="/notification?page=1&pagesize=10" Match="NavLinkMatch.All" Icon="@Icons.Material.Outlined.History">
			@Translations.History
		</MudNavLink>

	</MudNavGroup>

	<MudNavGroup Title="@Translations.Regulations" Icon="@Icons.Material.Filled.TextSnippet" Expanded="false">
		<MudNavLink Href="/terms" Match="NavLinkMatch.All" Icon="@Icons.Material.Filled.Policy">
			@Translations.Terms
		</MudNavLink>
		<MudNavLink Href="/privacy" Match="NavLinkMatch.All" Icon="@Icons.Material.Filled.PrivacyTip">
			@Translations.PrivacyPolicyTitle
		</MudNavLink>
	</MudNavGroup>

</MudNavMenu>